{
  "name": "@tsdotnet/threading",
  "version": "1.0.18",
  "description": "Classes and functions for simplifying task scheduling and distributing tasks to workers.",
  "author": "electricessence",
  "license": "MIT",
  "types": "dist/threading.d.ts",
  "main": "dist/threading.js",
  "module": "dist-esm/threading.js",
  "keywords": [
    "threading",
    "tasks",
    "task",
    "TaskHandler",
    "task handler",
    "task-handler",
    "handler",
    "delegate",
    "cancellable",
    "defer",
    "deferImmediate",
    "defer immediate",
    "defer-immediate",
    "worker",
    "web-worker"
  ],
  "scripts": {
    "build:dist": "rimraf dist/* && tsc && copyfiles -u 1 src/**/*.d.ts dist",
    "build:dist-esm": "rimraf dist-esm/* && tsc -p tsconfig.esm.json",
    "build": "pnpm i && pnpm run build:dist && pnpm run build:dist-esm",
    "bump": "npm run-script precommit && npm version patch",
    "docs": "rimraf docs/* && typedoc --options typedoc.json --readme none",
    "lint": "eslint src/**/*.ts",
    "precommit": "pnpm i && run-p lint test && run-p build:* && npm run-script validate && git status -s",
    "prepublishOnly": "npm run-script build && run-p validate test",
    "preversion": "run-p lint test",
    "postversion": "git push && git push --tags && npm run-script docs && git commit -m \"Updated docs.\" docs && git push",
    "test": "mocha -r ts-node/register tests/**/*.ts",
    "validate": "node ./.build/validate-package.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tsdotnet/threading.git"
  },
  "bugs": {
    "url": "https://github.com/tsdotnet/threading/issues"
  },
  "homepage": "https://github.com/tsdotnet/threading#readme",
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.17",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "chai": "^4.3.6",
    "copyfiles": "^2.4.1",
    "eslint": "^8.9.0",
    "eslint-config-typescript": "^3.0.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "mocha": "^9.2.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.5.0",
    "typedoc": "^0.22.11",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@tsdotnet/common-interfaces": "^1.0.4",
    "@tsdotnet/disposable": "^1.1.23",
    "@tsdotnet/exceptions": "^1.0.18",
    "@tsdotnet/lazy": "^1.0.24",
    "@tsdotnet/linked-node-list": "^1.3.19",
    "@tsdotnet/object-pool": "^1.0.8",
    "@tsdotnet/observable-base": "^1.0.20",
    "@tsdotnet/queue": "^1.2.7",
    "tslib": "^2.3.1"
  }
}
